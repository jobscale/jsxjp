<!doctype html>
<html>
<head>
  <meta charset="utf-8">
  <title>List of User</title>
  <link rel='stylesheet' href='/css/style.css' />
  <link rel="stylesheet" href="/css/form.css">
  <%- include('../menu/head') %>
</head>
<body>
  <div class="s-content">
    <%- include('../partial/header') %>
    <h1>List of User</h1>
    <hr>
    <div><ol type="I"><%
      items.forEach(item => {%>
        <li>
          <ol class="horizontal">
            <% if (item.html) { %>
              <li class="li-left"><%= item.html %></li>
              <li class="li-center right small">
                <%= item.count || 0 %> <%= item.deletedAt ? 'allow' : 'blocked' %><br>
                <%= item.lastAccess ? new Date(item.lastAccess).toISOString().replace(/T/, ' ').replace(/\..+$/, ' GMT') : '-' %>
              </li>
            <% } else { %>
              <li class="li-left"><%= item.login %></li>
              <li class="li-center right small">
                <%= item.deletedAt ? 'allow' : 'blocked' %><br>
                <%= item.lastAccess ? new Date(item.lastAccess).toISOString().replace(/T/, ' ').replace(/\..+$/, ' GMT') : '-' %>
              </li>
            <% } %>
            <li class="li-right"><a class="remove-button" data-id="<% item.key %>">DEL</a></li>
          </ol>
        </li>
      <%})
    %></ol></div>
  </div>
  <%- include('../menu/body') %>
</body>
</html>
